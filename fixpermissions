cd ramdisk
mkdir -p data
chmod -R 771 data

mkdir -p dev
chmod -R 755 dev

mkdir -p lib/modules
chmod -R 755 lib
cd lib/modules
chmod 644 *
cd ../..

mkdir -p proc
chmod -R 755 proc

mkdir -p res/customconfig/actions
mkdir -p res/images/charger
mkdir -p res/misc
chmod -R 755 res

cd res
chmod 644 uci.sh

cd customconfig
chmod 644 *
chmod 755 -R actions
cd actions
chmod 644 *
cd ../..

cd images/charger
chmod 644 *
cd ../..

cd misc
chmod 644 02modules
chmod 644 90userinit
chmod 644 bootmenu.zip
chmod 644 compcache
chmod 644 post_boot.sh
chmod 644 power.piranha.so
chmod 644 su
chmod 644 version.txt

cd init.41
chmod 644 *
chmod 755 -R sbin
cd sbin
chmod 644 *
chmod 755 -R boot
cd boot
chmod 644 *
cd ../../..

cd init.42
chmod 644 *
chmod 755 -R sbin
cd sbin
chmod 644 *
chmod 755 -R boot
cd boot
chmod 644 *
cd ../../..

