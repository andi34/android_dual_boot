chmod 644 *
cd ../../..

cd ../..

mkdir sbin
chmod -R 750 sbin
cd sbin
chmod 750 adbd
chmod 750 aroma
chmod 750 bootmenu/sh
chmod 750 busybox
chmod 750 charger
chmod 750 fstab.sh
chmod 750 healthd
chmod 750 init
chmod 750 init2
chmod 750 killer.sh
chmod 750 lpm
